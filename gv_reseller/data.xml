        <record id="product_att_year" model="product.attribute">
            <field name="name">Year</field>
            <field name="type">radio</field>
        </record>

        <record id="product_att_year_1" model="product.attribute.value">
            <field name="name">1</field>
            <field name="price_extra">50.00</field>
            <field name="attribute_id" eval="product_att_year"></field>
        </record>
        <record id="product_att_year_2" model="product.attribute.value">
