        <record id="product_pos" model="product.template">
            <field name="name">Point of Sale System</field>
            <field name="website_published">true</field>
            <field name="type">module</field>
            <field name="list_price">100.00</field>
            <field name="attribute_line_ids" eval="[(0, 0, { 'attribute_id': product_att_year, 'value_ids': [(6, 0, [product_att_year_1, product_att_year_2, product_att_year_3, ])] }), ]"></field>
