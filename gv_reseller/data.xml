        </record>
        <record id="product_impl_remote" model="product.attribute.value">
            <field name="name">Remote</field>
            <field name="attribute_id" eval="product_att_impl"></field>
        </record>

        <!-- PACKAGE VARIANTS -->
        <record id="product_att_package" model="product.attribute">
            <field name="name">Package</field>
            <field name="type">radio</field>
        </record>

        <record id="product_pack_a" model="product.attribute.value">
            <field name="name">A</field>
            <field name="attribute_id" eval="product_att_package"></field>
        </record>
        <record id="product_pack_b" model="product.attribute.value">
